}
//鼠标按下样式
QPushButton:checked
{
    background-color:rgb(255,255,255);//背景色（也可以设置图片）
    border-radius:10px;                   //边框圆角半径像素
    font:20px &quot;Segoe UI Black&quot;;                       //字体，字体大小
    color:rgb(253, 127, 151);                //字体颜色
}
//鼠标悬停样式
QPushButton:hover
{
    border-style:outset;                //边框样式（inset/outset）
    border-radius:1px;                  //边框圆角半径像素
    border-color:rgb(253, 127, 151);    //边框颜色
    background-color:rgb(255,255,255);
    font:23px &quot;Segoe UI Black&quot;;                      //字体，字体大小
    color:rgb(253, 127, 151);
}
